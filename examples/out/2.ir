FUNCTION fact :
PARAM un
IF un == #1 GOTO L0
v0 := #0
GOTO L1
LABEL L0 :
v0 := #1
LABEL L1 :
IF v0 != #0 GOTO L2
v1 := un - #1
ARG v1
v2 := CALL fact
v3 := un * v2
RETURN v3
GOTO L3
LABEL L2 :
RETURN un
LABEL L3 :

FUNCTION main :
READ um
IF um > #1 GOTO L4
v5 := #0
GOTO L5
LABEL L4 :
v5 := #1
LABEL L5 :
IF v5 != #0 GOTO L6
uresult := #1
GOTO L7
LABEL L6 :
ARG um
uresult := CALL fact
LABEL L7 :
WRITE uresult
RETURN #0

